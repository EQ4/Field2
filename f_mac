#!/bin/bash
pushd $(dirname `which "$0"`) >/dev/null; fieldhome="$PWD"; popd >/dev/null
echo $fieldhome

out=$fieldhome/out/production/

echo $fieldhome $out

#/usr/bin/java 
#/Library/Java/JavaVirtualMachines/jdk1.8.0_20.jdk/Contents/Home/bin/java 
/usr/bin/java -DappDir=$fieldhome -agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005 -Xmx4g -Xms4g -javaagent:$fieldhome/out/artifacts/fieldagent_jar/fieldagent.jar -cp `/usr/libexec/java_home`/lib/tools.jar:$fieldhome/lib/*:$fieldhome/out/artifacts/fieldagent_jar/fieldagent.jar:$out/fieldwork2/*:$out/fieldwork2/:$out/fielded/:$out/fielded/*:$out/fieldbox/:$out/fieldbox/*:$out/fieldnashorn/*:$out/fieldnashorn/ -Djava.library.path=$out/fieldwork2/macosx/ fieldagent.Trampoline ${*}
